
<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorRed</item>
        <item name="android:textColorHint">@color/colorWhite</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <!--Error label text style-->
    <style name="EditErrorText" parent="TextAppearance.AppCompat.Small">
        <item name="android:textColor">@color/colorRed</item>
    </style>

    <style name="EditRegTextTheme" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/colorWhiteOpacity</item>
        <item name="colorControlActivated">@color/colorWhite</item>
    </style>

    <style name="EditHintText" parent="TextAppearance.AppCompat.Small">
        <item name="android:textColor">@color/colorWhiteOpacity</item>
        <item name="android:textSize">@dimen/et_hint_text_size</item>
    </style>

    <style name="ToolbarBackArrowColor" parent="Theme.AppCompat.Light">
        <item name="android:textColorSecondary">@color/colorWhite</item>
    </style>




    <!--sdsad-->
    <style name="CircularProgress" parent="Theme.AppCompat.Light">
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!--CheckBox style-->
    <style name="CostomCheckBox" parent="Theme.AppCompat.Light">
        <item name="colorControlNormal">@color/colorWhite</item>
        <item name="colorControlActivated">@color/colorWhite</item>
    </style>
</resources>